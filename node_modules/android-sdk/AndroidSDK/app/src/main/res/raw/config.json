    "pathStyles":{
      "pathType":"line",
      "spacing":5,
      "pathWidth":5,
      "pathOpacity":0.5,
      "pathColor":"#f00",
      "additionalCss":"",
      "duration":3,
      "pathFocusPadding":50,
      "pathSegmentWidth":"5px",
      "pathSegmentColor":"#202020",
      "pathSegmentOpacity":0.5,
      "approxEntrance":false,
      "segmentOffset_x":0,
      "segmentOffset_y":0,
      "segmentOffset_zoom":0,
      "snapOffset_x":0,
      "snapOffset_y":0,
      "snapOffset_zoom":0
    },
    "popupStyles":{
      "css":{
        "width":"150px",
        "height":"40px",
        "background-color":"#202020",
        "color":"#FFFFFF"
      },
      "before":{

      },
      "after":{
        "width":"0",
        "height":"0",
        "border-left":"20px solid transparent",
        "border-right":"20px solid transparent",
        "border-top":"20px solid #f00"
      }
    },
    "popupCard":{
      "html":"<div class='mapInfoBox'><img class='startWayfindingIcon js-btn-startWayfinding' src='{{imgurl}}'><p class='popupCardStoreName'>{{name}}</p><p class='popupCardFloor'>{{floorName}}</p></div>'"
    },
    "svg":{
      "width":1920,
      "height":1920,
      "dropShadow":{
        "shadowStyle":{
          "opacity":0.15,
          "dx":0,
          "dy":8
        }
      },
      "darkness":{
        "opacity":0.5,
        "duration":0.5,
        "color":"#000000"
      },
      "textures":[

      ]
    },
    "iconStyles": {
      "pathIcon": {
        "useIcon": false
      },
      "youarehere": {
        "url": "images/yah.svg",
        "deepUrl": "",
        "width": 50,
        "height": 50,
        "allowZoomScale": false,
        "offset": {
          "x": -25,
          "y": -25
        }
      },
      "legends": {
        "height": 48,
        "width": 48
      },
      "destination":{
        "url":"images/pin.svg"
      }
    },

    "destinationHighlight":{
      "ease":"Back.easeInOut.config(1.7)"
    },
    "categoryHighlight":{
      "ease":"Back.easeInOut.config(1.7)"
    }
