{
  "mapStyles":{
    "exportStyles":{
      "Background-Layer":"show",
      "Parking-Lots-Layer":"show",
      "Mall-Boundary-Layer":"show",
      "Streets-SmallAlleys-Layer":"show",
      "Streets-Minor-Layer":"show",
      "Streets-Major-Layer":"show",
      "Obstacles-Layer":"show",
      "Units-Layer":"show",
      "Elevators-Layer":"show",
      "Escalators-Layer":"show",
      "LBox-Layer":"hide"
